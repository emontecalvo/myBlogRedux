{
  "name": "myBlog",
  "version": "0.0.0",
  "description": "a blogging app",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "build": "mkdir -p build && cp client/*.css client/*.html client/*.png build/ && webpack",
    "watch": "npm run build && run-p watch:*",
    "watch:copy": "chokidar \"client/*.css\" \"client/*.html\" \"client/*.png\" -c \"cp {path} build/\"",
    "watch:js": "webpack --watch",
    "watch:server": "nodemon -w server.js server.js -x \"npm run\"",
    "server.js": "npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emontecalvo/myBlogRouter.git"
  },
  "author": "Emily Monte Calvo",
  "license": "MIT",
  "homepage": "https://github.com/emontecalvo/myBlogRouter#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.2.0",
    "mongo": "^0.1.0",
    "mongodb": "^2.2.15",
    "mongoose": "^4.7.2",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "webpack": "^1.13.3"
  }
}
